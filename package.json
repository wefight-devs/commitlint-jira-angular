{
  "name": "@wefight-dev/commitlint-plugin-jira-angular",
  "description": "A commitlint plugin that permit to check if commit messages are respecting angular convention and contain at least 1 issue jira. example => git commit -m 'fix (JIR-42) : my commit message'",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Wefight",
  "keywords": [
    "commitlint",
    "jira-commitlint",
    "jira-style",
    "jira",
    "angular-convention",
    "angular",
    "angular-commitlint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wefight-devs/commitlint-jira-angular.git"
  },
  "bugs": {
    "url": "https://github.com/wefight-devs/commitlint-jira-angular/issues"
  },
  "main": "./build/src/index.js",
  "scripts": {
    "test": "TEST=true jest",
    "build": "tsc --project tsconfig.build.json",
    "transpile": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.0.0",
    "jest-junit": "^13.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "ts-node": "^10.4.0"
  },
  "peerDependencies": {
    "@commitlint/lint": ">=7.6.0"
  },
  "homepage": "https://github.com/wefight-devs/commitlint-jira-angular#readme",
  "dependencies": {}
